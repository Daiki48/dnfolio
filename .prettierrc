{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"tabWidth": 2,
	"endOfLine": "lf",
	"semi": true,
	"bracketSpacing": true,
	"bracketSameLine": false
}
